<% include ../partials/header %>
  <div class="container">


    <form action="/" method="post">
      <div class="form-group ">
        <label class='control-label' for="carName">Car Name:-</label>
        <input class="form-control" type="text" name="carName" placeholder='Enter your car name' value="">
      </div>
      <div class="form-group">
        <label class='control-label' for="carPrice">Car Price:-</label>
        <input class="form-control" type="number" name="carPrice" placeholder='Enter your car price' value="">
      </div>
      <div class="form-group">
        <label class='control-label' for="img">Url-</label>
        <input class="form-control" type="text" name="img" placeholder='car Image url' value="">
      </div>
      <div class="form-group">
        <label class='control-label' for="info">About:-</label>
        <input class="form-control" type="text" name="info" placeholder='about your car' value="">
      </div>
      <button type="submit" class='btn btn-primary'>Upload</button>

    </form>

  </div>



<% include ../partials/footer %>
