<% include ../partials/header %>
  <div class="container">


    <form action="/<%= Cars.id %>?_method=PUT" method="POST">
      <div class="form-group">
        <label class='control-label' for="carName">Car Name:-</label>
        <input class='form-control'type="text"  placeholder='Enter your car name' name='carName' value='<%= Cars.carName %>'>
      </div>
      <div class="form-group">
        <label class='control-label' for="carPrice">Car Price:-</label>
        <input class='form-control' type="number" placeholder='Enter your car price' name='carPrice'  value='<%= Cars.carPrice %>'>
      </div>
      <div class="form-group">
        <label class='control-label' for="img">Url-</label>
        <input class='form-control' type="text"  placeholder='car Image url' name='img' value= '<%= Cars.img %>'>
      </div>
      <div class="form-group">
        <label class='control-label' for="info">About:-</label>
        <input class='form-control' type="text"  placeholder='about your car' name='info' value='<%= Cars.info %>'>
      </div>
      <button type="submit" class='btn btn-primary'>Update</button>
    </form>

  </div>



<% include ../partials/footer %>
