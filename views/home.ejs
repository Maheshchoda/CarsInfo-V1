  <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img class="d-block w-100" src="http://kb4images.com/images/sports-car-images/36793358-sports-car-images.jpg" alt="First slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="http://www.hd-freewallpapers.com/imgdown/ferrari-cars-images-800x400" alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="http://www.hd-freewallpapers.com/imgdown/lamborghini-cars-pictures-download-800x400" alt="Third slide">
    </div>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
    <div class="container">
      <div class="row">
        <% for( var i = 0; i < Cars.length; i++){ %>
        <div class="col col-sm-6 col-md-4">
          <div class="card-deck">
              <div class="card">
                <img class='card-img-top' src='<%= Cars[i].img %>'  alt='carsImage'>
               <div class="card-body">
                 <h3 class='card-title'><%= Cars[i].carName %></h3>
                 <p class='card-text'><%= Cars[i].carPrice %></p>
                 <a href="/<%= Cars[i]._id %>"><button class='btn btn-info'type="button" name="button">Info</button></a>
               </div>
              </div>
          </div>
        </div>
           <% } %>
      </div>
   </div>
<% include ./partials/footer %>
